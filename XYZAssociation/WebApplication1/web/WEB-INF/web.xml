<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AuthenticateUser</servlet-name>
        <servlet-class>controller.AuthenticateUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthenticateUser</servlet-name>
        <url-pattern>/AuthenticateUser.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>RegistrationController</servlet-name>
        <servlet-class>controller.RegistrationController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationController</servlet-name>
        <url-pattern>/RegisterUser.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CheckCookie</servlet-name>
        <servlet-class>controller.CheckCookie</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckCookie</servlet-name>
        <url-pattern>/checkcookie.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ShowMemberInfo</servlet-name>
        <servlet-class>controller.ShowMemberInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowMemberInfo</servlet-name>
        <url-pattern>/ShowMemberInfo.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeClaimController</servlet-name>
        <url-pattern>/MakeClaimController.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MakeClaimController</servlet-name>
        <servlet-class>controller.MakeClaimController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MakePaymentController</servlet-name>
        <url-pattern>/MakePaymentController.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MakePaymentController</servlet-name>
        <servlet-class>controller.MakePaymentController</servlet-class>
    </servlet>
    <session-config>
        <session-timeout>
            20
        </session-timeout>
    </session-config>
</web-app>
